{
    "manifest_version": 3,
    "name": "CoRead",
    "description":"A simple reader for the web.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "icons":{
    
    },

    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
 
        }   
    }
    
}